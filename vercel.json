{
    "version": 2,
    "builds": [
        {
            "src": "API/admin_panel.py",
            "use": "@vercel/python"
        },
        {
            "src": "templates/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/API/admin_panel.py"
        }
    ]
}
